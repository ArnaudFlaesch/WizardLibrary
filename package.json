{
  "name": "wizard-library",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "16.x",
    "npm": "7.x"
  },
  "scripts": {
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "npm run test -- --code-coverage",
    "cy:open": "cypress open",
    "cy:verify": "npx cypress verify",
    "cy:run": "wait-on http://localhost:4200 && cypress run --headless --browser chrome",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json"
  },
  "dependencies": {
    "@angular/animations": "12.2.6",
    "@angular/cdk": "12.2.6",
    "@angular/common": "12.2.6",
    "@angular/compiler": "12.2.6",
    "@angular/core": "12.2.6",
    "@angular/forms": "12.2.6",
    "@angular/material": "12.2.6",
    "@angular/platform-browser": "12.2.6",
    "@angular/platform-browser-dynamic": "12.2.6",
    "@angular/router": "12.2.6",
    "bootstrap": "5.1.1",
    "class-transformer": "0.4.0",
    "date-fns": "2.24.0",
    "material-design-icons": "3.0.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.3.0",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.6",
    "@angular/cli": "12.2.6",
    "@angular/compiler-cli": "12.2.6",
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@types/jasmine": "3.9.1",
    "@types/node": "14.17.17",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "cypress": "8.4.1",
    "cypress-localstorage-commands": "1.5.0",
    "cypress-wait-until": "1.7.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.12.1",
    "gh-pages": "3.2.3",
    "jasmine-core": "3.9.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-firefox-launcher": "2.1.1",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "source-map-support": "0.5.20",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "ts-loader": "9.2.6",
    "ts-node": "10.2.1",
    "typescript": "4.2.3",
    "wait-on": "6.0.0"
  }
}
