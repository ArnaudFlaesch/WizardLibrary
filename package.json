{
  "name": "wizard-library",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "scripts": {
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "yarn run test -- --watch",
    "test:e2e": "ng e2e",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:verify": "cypress verify",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "yarn run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:report": "yarn run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "compodoc": "npx compodoc -p tsconfig.doc.json"
  },
  "dependencies": {
    "@angular/animations": "13.3.4",
    "@angular/cdk": "13.3.4",
    "@angular/common": "13.3.4",
    "@angular/compiler": "13.3.4",
    "@angular/core": "13.3.4",
    "@angular/forms": "13.3.4",
    "@angular/material": "13.3.4",
    "@angular/platform-browser": "13.3.4",
    "@angular/platform-browser-dynamic": "13.3.4",
    "@angular/router": "13.3.4",
    "bootstrap": "5.1.3",
    "class-transformer": "0.5.1",
    "date-fns": "2.28.0",
    "material-design-icons": "3.0.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.5",
    "tslib": "2.4.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.3",
    "@angular/cli": "13.3.3",
    "@angular/compiler-cli": "13.3.4",
    "@compodoc/compodoc": "1.1.19",
    "@cypress/schematic": "1.6.0",
    "@ngneat/spectator": "10.0.1",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.27",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "cypress": "9.5.4",
    "cypress-localstorage-commands": "1.7.0",
    "cypress-wait-until": "1.7.2",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "gh-pages": "3.2.3",
    "jasmine-core": "4.1.0",
    "jest": "27.5.1",
    "jest-preset-angular": "11.1.1",
    "prettier": "2.6.2",
    "stylelint": "14.7.1",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "3.0.0",
    "typescript": "4.6.3"
  }
}
